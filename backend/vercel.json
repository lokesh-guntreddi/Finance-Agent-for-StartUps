{
    "version": 2,
    "builds": [
        {
            "src": "backend/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/server.js"
        },
        {
            "src": "/run-analysis",
            "dest": "backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "backend/server.js"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb_uri",
        "PYTHON_BACKEND_URL": "@python_backend_url"
    }
}